{
  "name": "Startpage",
  "description": "A customizable new tab page",
  "version": "1.1.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "incognito": "split",
  "permissions": ["tabs", "contentSettings", "storage"],
  "chrome_url_overrides": {
    "newtab": "newTab.html"
  },
  "commands": {
    "toggle-pin": {
      "suggested_key": { "default": "Alt+P" },
      "description": "Toggle Pinned State"
    }
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://localhost:*/*", "https://evankaestner.github.io/startpage/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
